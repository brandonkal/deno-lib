{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"module": "esnext",
		"noEmit": true,
		"removeComments": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"sourceMap": true,
		"target": "es2017",
		"isolatedModules": false,
		"strict": true,
		"noImplicitAny": true,
		"typeRoots": []
	}
}
